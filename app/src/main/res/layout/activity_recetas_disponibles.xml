<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RecetasDisponibles">

    <ImageButton
        android:id="@+id/Camarones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:onClick="irCamarones"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/Spaguetti"
        app:layout_constraintStart_toStartOf="@+id/Spaguetti"
        app:srcCompat="@drawable/capture6" />

    <ImageButton
        android:id="@+id/Spaguetti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="229dp"
        android:onClick="irSpaguetti"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/capture7" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginBottom="50dp"
        android:text="Recetas Disponibles:"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/Camarones"
        app:layout_constraintStart_toStartOf="@+id/Camarones" />
</androidx.constraintlayout.widget.ConstraintLayout>